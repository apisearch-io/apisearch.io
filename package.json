    "webpack-cli": "^3.0.8",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "apisearch": "^0.2.11",
    "apisearch-ui": "^0.2.5",
    "bootstrap": "4.0.0-beta.2",
    "highlight.js": "^9.12.0",
    "jquery": "^3.3.1",
    "lodash": "^4.17.5",
